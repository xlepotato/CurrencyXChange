@model List<string>
@{
       Layout = "/Views/Shared/HeaderWithButtons.cshtml";
}


<div class="form-group">
    <input type="text" class="form-control" id="tbExchangeFrom" value="1">
</div>
<div class="form-group">
    <label for="sel1">Select list:</label>
    <select class="form-control" id="selExchangeFrom">
        @foreach (var x in Model)
        {
            <option>@x</option>
        }
    </select>
</div>
<div class="form-group">
    <input type="text" class="form-control" id="tbExchangeTo" value="1">
</div>
<div class="form-group">
    <label for="sel1">Select list:</label>
    <select class="form-control" id="selExchangeTo">
        @foreach (var x in Model)
        {
            <option>@x</option>
        }
    </select>
</div>
